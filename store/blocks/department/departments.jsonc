  "store.custom#departments": {
    "title": "Página de departamento",
    "children": ["flex-layout.row#department-container"]
  },
  "flex-layout.row#department-container": {
    "props": {
      "blockClass": "department-container",
      "fullWidth": true,
      "preventHorizontalStretch": true
    },
    "children": [
      "flex-layout.row#department-breadcrumb",
      "flex-layout.row#categories-container",
      "flex-layout.col#department-banner",
      "flex-layout.row#product-list-department-1",
      "flex-layout.row#product-list-department-2",
      "flex-layout.row#product-list-department-3",
