    }
  },
  "image#deparment-banner-4": {
    "props": {
      "src": "/arquivos/department-banner-4.png",
      "link": {
        "url": "/"
      },
      "maxHeight": 400,
      "blockClass": "deparment-banner"
    }
  },
  "flex-layout.row#product-list-department-1": {
    "title": "Slider 1",
    "children": ["flex-layout.col#product-list-department-1"],
    "props": {
      "blockClass": "product-list-department"
    }
  },
  "flex-layout.col#product-list-department-1": {
    "children": [
      "rich-text#department-product-list-title-1",
      "list-context.product-list#department-slider-1"
    ]
  },
  "rich-text#department-product-list-title-1": {
    "title": "Título slider 1",
    "props": {
      "blockClass": "deparment-product-list-title",
      "text": "Novidades"
    }
  },
  "list-context.product-list#department-slider-1": {
    "blocks": ["product-summary.shelf"],
    "children": ["slider-layout#department-slider-1"]
  },
  "slider-layout#department-slider-1": {
    "props": {
      "itemsPerPage": {
        "desktop": 5,
        "tablet": 3,
        "phone": 2
      },
      // "showNavigationArrows": "DesktopOnly",
      "showPaginationDots": "never",
      "blockClass": ["main-slider-home2"],
      "title": "Configuração - Slider 1"
    }
  },
