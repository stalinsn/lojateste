      "flex-layout.row#product-list-department-5",
      "flex-layout.row#product-list-department-6"
    ]
  },
  "flex-layout.row#department-breadcrumb": {
    "title": "Container Breadcrumb",
    "props": {
      "blockClass": "department-breadcumb-container",
      "fullWidth": true
    },
    "children": [
      "rich-text#department-breadcrumb-home",
      "rich-text#department-breadcrumb"
    ]
  },
  "rich-text#department-breadcrumb-home": {
    "title": "Home",
    "props": {
      "text": "[Home](/)",
      "blockClass": ["deparment-breadcrumb"]
    }
  },
  "rich-text#department-breadcrumb": {
    "title": "Breadcrumb do departamento",
    "props": {
      "text": "Eletro",
      "blockClass": ["deparment-breadcumb", "deparment-breadcrumb-current"]
    }
  },
  "flex-layout.row#categories-container": {
    "title": "Slider de categorias",
    "props": {
      "blockClass": "categories-container",
      "fullWidth": true,
      "preventHorizontalStretch": true
    },
    "children": ["flex-layout.col#categories-container"]
  },
  "flex-layout.col#categories-container": {
    "props": {
      "width": "100%"
    },
    "children": [
      "rich-text#categories-title",
      "list-context.info-card-list#department"
    ]
  },
  "rich-text#categories-title": {
    "title": "Título Categorias em destaque",
    "props": {
      "text": "#### Categorias em destaque \n Navegue pelas categorias mais acessadas",
      "blockClass": "categories-title"
    }
  },

