{
  "store.custom#departments": {
    "title": "Página de departamento",
    "children": ["flex-layout.row#department-container"]
  },
  "flex-layout.row#department-container": {
    "props": {
      "blockClass": "department-container",
      "fullWidth": true,
      "preventHorizontalStretch": true
    },
    "children": [
      "flex-layout.row#department-breadcrumb",
      "flex-layout.row#categories-container",
      "flex-layout.col#department-banner",
      "flex-layout.row#product-list-department-1",
      "flex-layout.row#product-list-department-2",
      "flex-layout.row#product-list-department-3",
      "image#deparment-banner-5",
      "flex-layout.row#product-list-department-4",
      "flex-layout.row#product-list-department-5",
      "flex-layout.row#product-list-department-6"
    ]
  },
  "flex-layout.row#department-breadcrumb": {
    "title": "Container Breadcrumb",
    "props": {
      "blockClass": "department-breadcumb-container",
      "fullWidth": true
    },
    "children": [
      "rich-text#department-breadcrumb-home",
      "rich-text#department-breadcrumb"
    ]
  },
  "rich-text#department-breadcrumb-home": {
    "title": "Home",
    "props": {
      "text": "[Home](/)",
      "blockClass": ["deparment-breadcrumb"]
    }
  },
  "rich-text#department-breadcrumb": {
    "title": "Breadcrumb do departamento",
    "props": {
      "text": "Eletro",
      "blockClass": ["deparment-breadcumb", "deparment-breadcrumb-current"]
    }
  },
  "flex-layout.row#categories-container": {
    "props": {
      "blockClass": "categories-container",
      "fullWidth": true
    },
    "children": ["rich-text#categories-title"]
  },
  "rich-text#categories-title": {
    "title": "Título Categorias em destaque",
    "props": {
      "text": "#### Categorias em destaque \n Navegue pelas categorias mais acessadas"
    }
  },

  /* list */

  "flex-layout.col#department-banner": {
    "title": "Banner Container",
    "props": {
      "blockClass": "deparment-banner-title"
    },
    "children": [
      "rich-text#deparment-banner-title",
      "flex-layout.row#deparment-banner-row"
    ]
  },
  "rich-text#deparment-banner-title": {
    "title": "Título",
    "props": {
      "blockClass": "deparment-banner-title",
      "text": "AQUELE SEU ELETRO E ELETRÔNICO VOCÊ ENCONTRA AQUI!"
    }
  },
  "flex-layout.row#deparment-banner-row": {
    "title": "Container dos banners",
    "children": [
      "image#deparment-banner-1",
      "image#deparment-banner-2",
      "image#deparment-banner-3",
      "image#deparment-banner-4"
    ],
    "props": {}
  },
  "image#deparment-banner-1": {
    "props": {
      "src": "/arquivos/department-banner-1.png",
      "link": {
        "url": "/"
      },
      "maxHeight": 400,
      "blockClass": "deparment-banner"
    }
  },
  "image#deparment-banner-2": {
    "props": {
      "src": "/arquivos/department-banner-2.png",
      "link": {
        "url": "/"
      },
      "maxHeight": 400,
      "blockClass": "deparment-banner"
    }
  },
  "image#deparment-banner-3": {
    "props": {
      "src": "/arquivos/department-banner-3.png",
      "link": {
        "url": "/"
      },
      "maxHeight": 400,
