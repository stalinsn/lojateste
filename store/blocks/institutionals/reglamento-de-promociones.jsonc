  },

  "rich-text#pregunta-13-6-pagina-reglamento-de-promociones": {
    "props": {
      "text": "Territorio",
      "blockClass": "estilo-titulo-disclosure-promos"
    }
  },
  "rich-text#answer-pregunta-13-6-pagina-reglamento-de-promociones": {
    "props": {
      "blockClass": "estilo-contenido-disclosure-promos",
      "text": "La presente promoción será válida y surtirá efectos únicamente dentro de Costa Rica."
    }
